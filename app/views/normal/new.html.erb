<div class="page-header"><h1>Upload Photo</h1></div>

<%= form_for @evento, :url => '/normal', html: { multipart: true } do |f| %>

  <div class="form-group">
    <label><input type="text" name="nombre" id="title" class="form-control"> Titulo</label>
  </div>

  <div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image, class: 'form-control' %>
  </div>

  <div class="form-group">
        <%= f.submit 'Upload Photo', class: 'btn btn-default' %>
    </div>
<% end %>